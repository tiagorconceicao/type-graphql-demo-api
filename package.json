{
  "name": "aurora_store_demo",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "tsnd --transpile-only --ignore-watch node_modules --respawn src/index.ts",
    "build": "tsc -b",
    "typeorm": "node -r ts-node/register ./node_modules/typeorm/cli.js",
    "typeorm:migrate": "yarn typeorm migration:run",
    "typeorm:drop": "yarn typeorm schema:drop",
    "typeorm:seed": "ts-node ./node_modules/typeorm-seeding/dist/cli.js seed",
    "typeorm:refresh": "npm-run-all typeorm:drop typeorm:migrate",
    "typeorm:refresh:seed": "npm-run-all typeorm:drop typeorm:migrate typeorm:seed",
    "seed:config": "ts-node ./node_modules/typeorm-seeding/dist/cli.js config",
    "pretest": "del ./__tests__/database.sqlite; NODE_ENV=test node -r ts-node/register -r dotenv/config ./node_modules/typeorm/cli.js migration:run dotenv_config_path=.env.test",
    "test": "jest",
    "posttest": "del ./__tests__/database.sqlite"
  },
  "dependencies": {
    "apollo-server-express": "^2.17.0",
    "argon2": "^0.27.0",
    "class-validator": "^0.12.2",
    "connect-redis": "^5.0.0",
    "cpf-cnpj-validator": "^1.0.3",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "express-validation": "^3.0.6",
    "graphql": "^15.3.0",
    "morgan": "^1.10.0",
    "mysql2": "^2.1.0",
    "npm-run-all": "^4.1.5",
    "redis": "^3.0.2",
    "reflect-metadata": "^0.1.10",
    "type-graphql": "^1.0.0",
    "typeorm": "0.2.25",
    "typeorm-seeding": "^1.6.1",
    "uuid": "^8.3.0"
  },
  "devDependencies": {
    "@types/connect-redis": "^0.0.14",
    "@types/express": "^4.17.8",
    "@types/express-session": "^1.17.0",
    "@types/faker": "^5.1.0",
    "@types/jest": "^26.0.13",
    "@types/morgan": "^1.9.1",
    "@types/node": "^8.0.29",
    "@types/redis": "^2.8.27",
    "@types/uuid": "^8.3.0",
    "@types/validator": "^13.1.0",
    "del-cli": "^3.0.1",
    "jest": "^26.4.2",
    "sqlite3": "^5.0.0",
    "ts-jest": "^26.3.0",
    "ts-node": "^9.0.0",
    "ts-node-dev": "^1.0.0-pre.62",
    "typescript": "^4.0.2"
  },
  "prettier": {
    "printWidth": 100
  }
}
